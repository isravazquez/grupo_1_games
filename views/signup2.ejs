<%- include("./partials/head") %>
<%- include("./partials/header") %>
<div class="login-container">
    <div class="loginform-container">
        <div class="loginleft"></div>
        <div class="loginright">
            <form action="/users/create" method="POST" id="signupform">
                <h1>Registro</h1>
                <input type="text" name="email" id="email" placeholder="Introduce tu correo electrónico">
                <input type="text" name="name" id="name" placeholder="Introduce tu nombre">
                <input type="text" name="lastname" id="lastname" placeholder="Introduce tus apellidos">
                <input type="password" name="password" id="password" placeholder="Contraseña">
                <input type="file" name="image" id="image">
                <input type="submit" value="ENVIAR" id="conectar" onclick="">
                <a href="/login" id="login">Ya tengo cuenta</a>
            </form>
        </div>
    </div>
</div>